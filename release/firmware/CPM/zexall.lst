 183:				; jgh: macro syntax changed for ZMAC and MAXAM
 184:				;	can't use opcodes as labels
 185:				;	ZMAC allows &nn as hex, so & removed from local labels
 186:				;
 187:				tstr	macro	insn1,insn2,insn3,insn4,memop,riy,rix,rhl,rde,rbc,flags,acc,rsp,?lab
 188:				?lab:	db	insn1,insn2,insn3,insn4
 189:					dw	memop,riy,rix,rhl,rde,rbc
 190:					db	flags
 191:					db	acc
 192:					dw	rsp
 193:					if	$-?lab ne 20
 194:					error	'missing parameter'
 195:					endif
 196:					endm
 197:				
 198:				tmsg	macro	msg,?lab
 199:				?lab:	db	msg
 200:					if	$ ge ?lab+31
 201:					error	'message too long'
 202:					else
 203:				;	ds	?lab+30-$,'.'	; jgh: ZMAC/MAXAM don't have char parameter
 204:					endif
 205:					db	'$'
 206:					endm
